grammar uk.ac.kcl.inf.languages.tracery.TraceryLanguage with org.eclipse.xtext.common.Terminals

generate traceryLanguage "http://www.ac.uk/kcl/inf/languages/tracery/TraceryLanguage"

TraceryProgram:
	'{'
	statements += Statement*
	'}'
;

Statement:
	'"' Variable '":' '['  ']'  
;

Variable:
	"text"
;
