<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="traceryLanguage" nsURI="http://www.ac.uk/kcl/inf/languages/tracery/TraceryLanguage"
    nsPrefix="traceryLanguage">
  <eClassifiers xsi:type="ecore:EClass" name="TraceryProgram">
    <eStructuralFeatures xsi:type="ecore:EReference" name="initialStatement" eType="#//InitialStatement"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="statements" upperBound="-1"
        eType="#//Statement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VariableDecleration" eSuperTypes="#//InnerStatement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InitialStatement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="startVal" upperBound="-1"
        eType="#//startValue" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="vals" upperBound="-1" eType="#//normalValue"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Statement"/>
  <eClassifiers xsi:type="ecore:EClass" name="startValue">
    <eStructuralFeatures xsi:type="ecore:EReference" name="innards" upperBound="-1"
        eType="#//InnerStatement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="normalValue">
    <eStructuralFeatures xsi:type="ecore:EReference" name="innards" upperBound="-1"
        eType="#//InnerStatement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InnerStatement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StartingJSONExpression" eSuperTypes="#//Statement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="startVal" upperBound="-1"
        eType="#//startValue" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="vals" upperBound="-1" eType="#//normalValue"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="var" eType="#//VariableDecleration"/>
  </eClassifiers>
</ecore:EPackage>
